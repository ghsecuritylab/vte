#!/bin/ksh
DEVICE=/dev/rrz5c
file $DEVICE
dtc $DEVICE d740-01.dtc 10000 "10240 5120 2048 1024 512" &
dtc $DEVICE d740-02.dtc 10000 "512 1024 2048 5120 10240" &
dtc $DEVICE d740-03.dtc 10000 "1024 2048 5120 10240 512" &
dtc $DEVICE d740-04.dtc 10000 "2048 5120 10240 512 1024" &
dtc $DEVICE d740-05.dtc 10000 "5120 10240 512 1024 2048" &

