#files: rtic_test
#files: test.txt


#RTIC testcases
TGE-LV-RTIC-MOD insmod $LTPROOT/testcases/bin/rtic_test_module.ko
TGE-LV-RTIC-0010 rtic_test -F $LTPROOT/testcases/bin/test.txt -M ONETIME -B A1 -v
TGE-LV-RTIC-0020 rtic_test -F $LTPROOT/testcases/bin/test.txt -M ONETIME -B B1 -v
TGE-LV-RTIC-0030 rtic_test -F $LTPROOT/testcases/bin/test.txt -M ONETIME -B C1 -v
TGE-LV-RTIC-0040 rtic_test -F $LTPROOT/testcases/bin/test.txt -M ONETIME -B D1 -v
TGE-LV-RTIC-0050 rtic_test -F $LTPROOT/testcases/bin/test.txt -M RUNTIME -B A1 -v
TGE-LV-RTIC-0060 rtic_test -F $LTPROOT/testcases/bin/test.txt -M RUNTIME -B B1 -v
TGE-LV-RTIC-0070 rtic_test -F $LTPROOT/testcases/bin/test.txt -M RUNTIME -B C1 -v
TGE-LV-RTIC-0080 rtic_test -F $LTPROOT/testcases/bin/test.txt -M RUNTIME -B D1 -v
TGE-LV-RTIC-0090 rtic_test -F $LTPROOT/testcases/bin/test.txt -M ONETIME -B A1 -I 1 -v
TGE-LV-RTIC-0100 rtic_test -F $LTPROOT/testcases/bin/test.txt -M RUNTIME -B A1 -I 1 -v
TGE-LV-RTIC-0110 rtic_test -C 
TGE-LV-RTIC-0120 rtic_test -D
TGE-LV-RTIC-0130 rtic_test -R 0
TGE-LV-RTIC-0140 rtic_test -R 1

