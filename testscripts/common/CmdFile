arecord -d 5 -c 1 -r 8000 /tmp/audio.wav
arecord -d 5 -c 1 -r 11000 /tmp/audio.wav
arecord -d 5 -c 1 -r 16000 /tmp/audio.wav
arecord -d 5 -c 1 -r 22000 /tmp/audio.wav
arecord -d 5 -c 1 -r 24000 /tmp/audio.wav
arecord -d 5 -c 1 -r 32000 /tmp/audio.wav
arecord -d 5 -c 1 -r 44000 /tmp/audio.wav
arecord -d 5 -c 1 -r 48000 /tmp/audio.wav
arecord -d 5 -c 1 -r 64000 /tmp/audio.wav
arecord -d 5 -c 1 -r 96000 /tmp/audio.wav

